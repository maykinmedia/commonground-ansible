env_file: "{{ _django_app_docker_app_user.home }}/dotenv"
env:
  DB_HOST: "{{ django_app_docker_db_host }}"
  DB_PORT: "{{ django_app_docker_db_port | string }}"
  SECRET_KEY: "{{ django_app_docker_secret_key }}"
{% if django_app_docker_use_celery and item.queue is defined %}
  CELERY_WORKER_QUEUE: "{{ item.queue }}"
{% endif %}
{% if django_app_docker_use_flower and django_app_docker_flower_url_prefix %}
  FLOWER_URL_PREFIX: "{{ django_app_docker_flower_url_prefix }}"
{% endif %}
